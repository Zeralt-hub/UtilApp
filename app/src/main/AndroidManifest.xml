<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Material3.Light"
        android:icon="@mipmap/utilapp">
        <activity
            android:name="com.xacarana.utilapp.AcercaDe"
            android:exported="false" />
        <activity
            android:name="com.xacarana.utilapp.Formulas2"
            android:exported="false" />
        <activity
            android:name="com.xacarana.utilapp.Formulas"
            android:exported="false" />
        <activity
            android:name="com.xacarana.utilapp.calculadora"
            android:exported="false" />
        <activity
            android:name="com.xacarana.utilapp.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.xacarana.utilapp.Registro"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.xacarana.utilapp.Login"
            android:exported="false"
            android:theme="@style/Theme.Material3.Light.NoActionBar" />
        <activity
            android:name="com.xacarana.utilapp.MainActivity"
            android:exported="true" />
    </application>

</manifest>